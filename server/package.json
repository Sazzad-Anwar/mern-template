{
    "name": "server",
    "version": "1.0.1",
    "description": "",
    "main": "app.js",
    "scripts": {
        "server-install": "yarn && yarn add -D nodemon morgan concurrently && yarn add axios bcryptjs cors dotenv express express-async-handler multer mysql mongoose sequelize uuid nanoid winston jsonwebtoken colors",
        "start": "node app.js",
        "server": "SET NODE_ENV=development & nodemon app.js",
        "client": "cd ../client && yarn start",
        "build-client": "cd ../client && yarn build",
        "client-install": "cd ../client && yarn && yarn lint && yarn install-packages",
        "install-all": "yarn server-install && yarn client-install",
        "dev": "concurrently \"yarn server\" \"cd ../client && yarn start\"",
        "docker-build-dev-start": "docker-compose -f docker-compose.yaml -f docker-compose.dev.yaml up -d --build",
        "docker-dev-start": "docker-compose -f docker-compose.yaml -f docker-compose.dev.yaml up -d",
        "docker-dev-stop": "docker-compose -f docker-compose.yaml -f docker-compose.dev.yaml down",
        "docker-prod-build-start": "docker-compose -f docker-compose.yaml -f docker-compose.prod.yaml up -d --build",
        "docker-prod-start": "docker-compose -f docker-compose.yaml -f docker-compose.prod.yaml up -d",
        "docker-prod-stop": "docker-compose -f docker-compose.yaml -f docker-compose.prod.yaml down",
        "request-handle-check": "autocannon -c 10 http://localhost:8080/api/v1/checkStatus"
    },
    "keywords": [
        "MERN Framework, redux, node, express, sequelize"
    ],
    "author": "Md Sazzad Bin Anwar",
    "license": "ISC",
    "dependencies": {
        "auth-middleware-jwt": "^1.0.5",
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "colors": "^1.4.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-async-handler": "^1.1.4",
        "helmet": "^4.6.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.13.2",
        "multer": "^1.4.2",
        "mysql": "^2.18.1",
        "nanoid": "^3.1.23",
        "sequelize": "^6.6.5",
        "uuid": "^8.3.2",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "concurrently": "^6.2.0",
        "morgan": "^1.10.0",
        "nodemon": "^2.0.11"
    }
}
