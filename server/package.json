        "mongoose": "^6.2.9",
        "multer": "^1.4.4",
        "mysql": "^2.18.1",
        "nanoid": "^3.3.2",
        "redis": "^4.0.4",
        "sequelize": "^6.17.0",
        "serve-favicon": "^2.5.0",
        "socket.io": "^4.4.1",
        "uuid": "^8.3.2",
        "winston": "^3.6.0"
    }
